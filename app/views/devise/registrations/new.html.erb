<div class="heading">
  <h2>Mogul Sign Up</h2>
</div>

<%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.input :profile_name, label: "Player name" %>
    <%= f.input :hometown %>
    <%= f.submit "Sign up" %>
<% end %>